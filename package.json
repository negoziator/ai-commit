        "execa": "^9.0.0",
        "fs-fixture": "^2.0.0",
        "https-proxy-agent": "^7.0.0",
        "ini": "^6.0.0",
        "kolorist": "^1.7.0",
        "manten": "^1.9.0",
        "micromatch": "^4.0.8",
